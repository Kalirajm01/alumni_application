        /* Option 2: Import via CSS */
@import url("https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css");
        body {
            font-family: Arial, sans-serif;
            background-color: #f4f4f4;
            margin: 0;
            padding: 0;
        }

        header {
            background-color: #0077b5;
            color: white;
            padding: 10px;
            text-align: center;
        }



        .alumni-card {
            display: flex;
            align-items: center;
            padding: 10px;
            border-bottom: 1px solid #ddd;
            position: relative;
        }
        .banner-image {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 60%;
            object-fit: cover;
            border-radius: 20px 20px 0 0; /* Rounded corners, adjust as needed */
        }
        .profile img {
            border-radius: 50%;
            margin-right: 15px;
        }

        .alumni-details {
            flex: 1;
            position: relative; /* Ensure details appear above the banner image */
            z-index: 1; /* Keep details above the banner image */
            padding: 10px;
        }

        .alumni-name {
            font-size: 1.2em;
            font-weight: bold;
        }

        .alumni-position {
            color: #555;
        }

      
        .recently-connected {
            max-width: 300px;
            justify-content: center;
            margin-left: 20px;
            /* Adjust margin as needed */
        }

        .recently-connected-item {
            margin-bottom: 20px;
        }

        .recently-connected-item img {
            border-radius: 50%;
            margin-right: 10px;
        }
        .side-page{
            justify-content: center;
            
            background-color: white;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            
            border-radius: 5px;
        }
        .card{
            padding: 20px;
        }
        .profile{
            position: relative;
        }
        h2 {
            border-bottom: 2px solid #0077b5; /* Blue color line with some thickness */
            padding-bottom: 5px; /* Adjust as needed */
            width: 25%;
        }

